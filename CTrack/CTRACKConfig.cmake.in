# CTRACK source dir
SET( CTRACK_SOURCE_DIR "@CMAKE_CURRENT_SOURCE_DIR@")

# CTRACK build dir
SET( CTRACK_DIR "@CMAKE_CURRENT_BINARY_DIR@")

